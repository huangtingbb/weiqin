.user-info {
  height: 346rpx;
  background: #277f65;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #fff;
}

.thumb {
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  margin: 60rpx 0rpx 36rpx 0rpx;
  border: 2rpx solid #fff;
}

.user-count {
  display: flex;
  padding: 15rpx 0rpx;
  background: #fff;
}

.user-money {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 375rpx;
  height: 130rpx;
}

.user-money:first-child {
  width: 374rpx;
  border-right: 1px solid #ececec;
}

.icon {
  width: 42rpx;
  height: 28rpx;
  margin-right: 24rpx;
}

.m_up {
  margin-top: 26rpx;
}

.num {
  font-size: 42rpx;
  color: #666;
  font-weight: 700;
}

.m_dowm {
  margin-top: 26rpx;
  display: flex;
  align-items: center;
}

.session {
  padding: 0rpx 0rpx 0rpx 20rpx;
  background: #fff;
  margin-top: 20rpx;
}

.session-list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ececec;
  padding-left: 10px;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.session-lat {
  border-bottom: 0px solid #ececec;
}

.session-list view {
  display: flex;
  align-items: center;
  color: #666;
}

.iconlist {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.foot {
  height: 100rpx;
}

.wave-bg {
  z-index: 1;
  animation: wave-bg 10.25s linear infinite;
}

.wave {
  position: absolute;
  z-index: 3;
  right: 0;
  bottom: 0;
  opacity: 0.725;
  height: 260rpx;
  width: 2250rpx;
  animation: wave 10s linear infinite;
}

.concat {
  background: #fff;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  margin-left: 0rpx;
  margin-right: 0rpx;
  padding-right: 0rpx;
  color: #666;
  border-bottom: 1px solid #ececec;
  padding-top: 20rpx;
  padding-bottom: 18rpx;
}

.concat :hover {
  background: #fff;
  border: 1px solid #fff;
}

.noBoderB{
  border-bottom: none !important;
}

.concat .iconlist{
  margin-left: -9rpx;
  margin-right: 24rpx
}

.right{
  width: 35rpx;
  height: 35rpx;
  margin-right: 15rpx;
}

.concat text{
  margin-right: auto;
}


button::after{
  border: none;
}

button::before{
  border: none;
}

.tdNfeedback{
  padding: 0;
  line-height:1;
  background: #fff;
  color:#666;
  font-size: 28rpx;
}