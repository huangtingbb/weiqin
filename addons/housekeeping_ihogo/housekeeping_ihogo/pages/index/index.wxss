swiper {
  height: 374rpx;
}

.slide-image {
  width: 750rpx;
  height: 374rpx;
}

.notice {
  width: 100%;
  height: 82rpx;
  background-color: #fff;
  line-height: 82rpx;
  font-size: 28rpx;
  color: #222;
  display: flex;
  align-items: center;
  white-space: nowrap;
}

.notice view {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.notice image {
  display: flex;
  width: 32rpx;
  height: 32rpx;
  margin: 0rpx 16rpx 0rpx 20rpx;
}

.notice .notice-msg {
  overflow: hidden;
  text-overflow: ellipsis;
  padding-right: 15rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 30rpx;
  font-weight: 500;
}

.userOpe {
  background-color: #fff;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.opeartion {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 23%;
  margin: 25rpx 1%;
}

.opeartion image {
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
}

.opeartion text {
  font-size: 24rpx;
  color: #222;
  margin-top: 20rpx;
}

.session {
  margin-top: 10rpx;
  padding: 40rpx 20rpx 24rpx;
  background-color: #fff;
}

.session-title {
  font-size: 32rpx;
  font-weight: 700;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.session-title:before {
  content: " ";
  display: block;
  width: 10rpx;
  height: 30rpx;
  background-color: #277f65;
  border-radius: 16rpx;
  margin-right: 14rpx;
}

.session-cont {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.session-list {
  width: 345rpx;
  height: 280rpx;
  overflow: hidden;
  position: relative;
  border-radius: 10rpx;
  margin-top: 24rpx;
  margin-bottom: 6rpx;
}

.session-list:nth-child(odd) {
  margin-right: 20rpx;
}

.service {
  width: 345rpx;
  height: 280rpx;
}

.session-list text {
  display: block;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  height: 64rpx;
  width: 100%;
  line-height: 64rpx;
  text-align: center;
  position: absolute;
  bottom: 0rpx;
}

.session-order {
  padding: 40rpx 20rpx 24rpx;
}

.session-wrap {
  display: flex;
  flex-direction: column;
  margin-top: 24rpx;
}

.session-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 30rpx;
  padding-bottom: 26rpx;
  border-bottom: 1px solid #ececec;
}

.item-left {
  display: flex;
  color: #999;
}

.order {
  width: 135rpx;
  height: 135rpx;
  border-radius: 5px;
  margin-right: 20rpx;
}

.item-title {
  font-size: 32rpx;
  line-height: 32rpx;
  margin-top: 6rpx;
  margin-bottom: 16rpx;
  color: #222;
}

.item-desc {
  width: 400rpx;
  overflow: hidden;
  line-height: 32rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.item-price {
  margin-top: 18rpx;
  font-size: 26rpx;
  font-weight: 700;
}

.item-right {
  height: 50rpx;
  width: 124rpx;
  border: 2rpx solid #277f65;
  border-radius: 4rpx;
  font-size: 26rpx;
  font-weight: 700;
  color: #277f65;
  line-height: 50rpx;
  text-align: center;
}

.rightBtnSSFor {
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  height: 135rpx;
  align-items: center;
}

.rightBtnSSFor .item-right image {
  height: 25rpx;
  width: 25rpx;
}

.allScereenShadows {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  overflow-y: hidden;
  top: 0;
  z-index: 120;
}

.popBox {
  background: #fff;
  width: 70%;
  margin: 0 auto;
  margin-top: 9%;
  border-radius: 5px;
  padding: 0 5%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  z-index: 121;
  padding-bottom: 0;
}

.applynewCenterBox {
  padding: 5% 5%;
  padding-top: 0;
}

.applynewCenterBox .title {
  padding-bottom: 7%;
  font-size: 43rpx;
  text-align: center;
  margin-top: 3%;
  font-weight: 600;
}

.btnBottom {
  align-items: center;
  display: flex;
  justify-content: space-between;
  padding: 0 5%;
  padding-bottom: 5%;
}

.allScereenShadows .cancle {
  color: #8a8a8a;
  font-size: 30rpx;
  margin-top: 3%;
}

.allScereenShadows .chooseTimeRight {
  background: #277f65;
  color: white;
  height: 80rpx;
  line-height: 80rpx;
  margin-left: auto;
  font-size: 30rpx;
  border-radius: 20px;
  width: 6rem;
  text-align: center;
  padding: 0;
  margin-right: 0;
}

.closeAllPop {
  color: #999;
  position: fixed;
  top: 3%;
  right: 8%;
  z-index: 999;
  padding: 5%;
  font-size: 45rpx;
}

.popBox .manName {
  font-size: 32rpx;
  text-align: center;
  margin-bottom: 25rpx;
}

.popBox .popBoxImage {
  margin-top: 15rpx;
  text-align: center;
}

.popBox .popBoxImage image {
  margin-right: 0;
  margin-bottom: 5rpx;
}

.moneyAll{
  font-size: 32rpx;
  padding: 10rpx 0;
}

.popBoxMainView{
  overflow-y: scroll;
}